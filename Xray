ToggleButton.Size = UDim2.new(0, 120, 0, 30)
ToggleButton.Font = Enum.Font.Gotham
ToggleButton.Text = "Xray: Off"
ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
ToggleButton.TextSize = 14



local ButtonCorner = Instance.new("UICorner")
ButtonCorner.CornerRadius = UDim.new(0, 8)
ButtonCorner.Parent = ToggleButton

ToggleButton.MouseEnter:Connect(function()
    ToggleButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
end)
ToggleButton.MouseLeave:Connect(function()
    ToggleButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
end)


local isXrayOn = false
local originalTransparencies = {}

local function toggleXray()
    isXrayOn = not isXrayOn
    if isXrayOn then
        ToggleButton.Text = "Xray: On"
        ToggleButton.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
        for _, part in ipairs(workspace:GetDescendants()) do
            if part:IsA("BasePart") and not part:IsDescendantOf(game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()) then
                if not originalTransparencies[part] then
                    originalTransparencies[part] = part.Transparency
                end
                part.Transparency = 0.5
            end
        end
    else
        ToggleButton.Text = "Xray: Off"
        ToggleButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        for part, trans in pairs(originalTransparencies) do
            if part and part.Parent then
                part.Transparency = trans
            end
        end
        originalTransparencies = {}
    end
end

ToggleButton.MouseButton1Click:Connect(toggleXray)


game.Players.LocalPlayer.CharacterAdded:Connect(function()
    if isXrayOn then
        wait(1)
        toggleXray()
        toggleXray()
    end
end)
